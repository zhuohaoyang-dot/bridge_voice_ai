# Server Configuration
PORT=3010
NODE_ENV=production

# Redis Configuration (Railway will provide this)
REDIS_URL=redis://localhost:6379
REDIS_REQUIRED=false
REDIS_CLEAN_ON_START=false

# Webhook Configuration
WEBHOOK_URL=https://your-app-name.railway.app/webhook

# VAPI Configuration
VAPI_API_KEY=your_vapi_api_key_here
VAPI_ASSISTANT_ID=your_vapi_assistant_id_here

# Twilio Configuration (for seamless transfers)
TWILIO_ACCOUNT_SID=your_twilio_account_sid_here
TWILIO_AUTH_TOKEN=your_twilio_auth_token_here
TWILIO_PHONE_NUMBER=your_twilio_phone_number_here

# Convoso Configuration (if using)
CONVOSO_API_KEY=your_convoso_api_key_here
CONVOSO_BASE_URL=https://api.convoso.com

# Security
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=1000

# Logging
LOG_LEVEL=info 